<% if customer_signed_in? %>
  <%= form_with url: customer_search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= f.select :model, options_for_select({ "user name" => "customer", "Diary" => "diary" }) %>
    <%= f.select :method, options_for_select({ "完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partial" }) %>
    <%= f.submit '検索',placeholder: "Search" %>
  <% end %>
<% end %>